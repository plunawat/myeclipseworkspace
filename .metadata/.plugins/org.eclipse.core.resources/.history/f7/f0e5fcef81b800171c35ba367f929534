package com.prl;

public class LamdaTest {
	public static void main(String[] args) {
		
		Calculator sum = (i1,i2) -> System.out.println("The sum of two number is " + (i1+i2));
		Calculator multiply = (i1,i2) -> System.out.println("Multiplication of two number is "+(i1*i2));
		
	}
}

interface Calculator	{
	public void mathOperation(int i1, int i2);
}
