package com.prl;

public class StringTest {
	public static void main(String[] args) {
		String str = "My name is parikshit";
		System.out.println(String.join("*", str.split("\\s+")));
		
		String s1 = "AAA";
		String s2 = new String("AAA");
		String s3 = "AAA";
		String s4 = "Parikshit Lunawat";
		
		System.out.println(s4.substring(5,8));
		
		System.out.println("ABCDE".charAt(3));
		
		s1.concat(s2).concat(s3);
		System.out.println(s1);
		
		System.out.println(s1 == s2);
		System.out.println(s1 == s3);
	}

}
